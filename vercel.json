{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/login",
      "dest": "server.js",
      "methods": ["POST"]
    },
    {
      "src": "/register",
      "dest": "server.js",
      "methods": ["POST"]
    }
  ]
}
